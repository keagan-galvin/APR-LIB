{
  "name": "apr-framework",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile-sample-css": "less-watch-compiler --config ./samples/less-watch-compiler.config.json",
    "doc:readme": "documentation build src/lib/** -f md -o README.md",
    "doc:html": "documentation build src/lib/** -f html -o docs",
    "doc": "npm-run-all -p doc:*",
    "css:compile-scss": "node-sass-chokidar --source-map true src/scss/ -o dist",
    "css:css-autoprefixer": "postcss dist/*.css --use autoprefixer -d dist",
    "css": "npm-run-all -p css:*",
    "prod:webpack": "webpack --config webpack.config.prod.js --mode production",
    "prod": "npm-run-all -p prod:* css:* doc:*",
    "dev:js": "webpack --config webpack.config.js --mode development",
    "dev": "npm-run-all -p dev:* css:*"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "autoprefixer": "^9.4.10",
    "babel-loader": "^8.0.2",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-preset-minify": "^0.5.0",
    "classlist-polyfill": "^1.2.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "datatables.net": "^1.10.19",
    "datatables.net-buttons": "^1.5.6",
    "datatables.net-dt": "^1.10.19",
    "import-loader": "^1.0.1",
    "jquery": "^3.3.1",
    "jszip": "^3.2.0"
  },
  "browserslist": [
    ">= .5%"
  ]
}
